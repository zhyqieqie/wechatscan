.stamp {
  margin: 0rpx;
  list-style: none;
  font-family: "Microsoft YaHei", 'Source Code Pro', Menlo, Consolas, Monaco, monospace;
  height: 230rpx;
  padding: 0rpx 10rpx;
  margin-top: 40rpx;
  position: relative;
  overflow: hidden;
  width: 95.5%;
}

.stamp::before {
  content: '';
  position: absolute;
  top: 0rpx;
  bottom: 0rpx;
  left: 20rpx;
  right: 20rpx;
  z-index: -1;
}

.stamp .par {
  float: left;
  margin-left: 20rpx;
  width: 400rpx;
  border-right: 2rpx dashed rgba(255, 255, 255, 0.3);
  text-align: left;
  height: 230rpx;
}

.stamp .par .yuan {
  float: left;
  font-size: 30rpx;
  color: #fff;
  line-height: 130rpx;
}

.stamp .par .jine {
  float: left;
  font-size: 60rpx;
  color: #fff;
  line-height: 110rpx;
  font-weight: bold;
}

.stamp .par .leixing {
  font-size: 30rpx;
  color: #fff;
  margin-right: 10rpx;
  line-height: 130rpx;
}

.stamp .par .tiaojian {
  color: #fff;
  font-size: 26rpx;
  line-height: 32rpx;
}

.stamp .par .sub {
  position: relative;
  top: -10rpx;
  color: rgba(255, 255, 255, 0.8);
}

.block {
  display: block;
  position: absolute;
}

.stamp .copy {
  display: inline-block; 
  width: 180rpx;
  vertical-align: text-bottom;
  font-size: 30rpx;
  color: rgb(255, 255, 255);
  text-align: center;
  line-height: initial;
  margin-top: 30rpx;
}

.stamp .copy p {
  font-size: 32rpx;
  margin-top: 30rpx;
}

.maxlength {
  height: 35rpx;
  overflow: hidden;
}

.stamp02 {
  background: radial-gradient(transparent 0, transparent 10rpx, #d24161 10rpx);
